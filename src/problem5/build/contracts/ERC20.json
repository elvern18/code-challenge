  "source": "pragma solidity ^0.8.3;\r\n\r\ninterface ERC20 {\r\n    function balanceOf(address account) external view returns (uint256);\r\n}\r\n\r\ncontract TokenBalances {\r\n\r\n    struct Amount {\r\n        address token;\r\n        uint256 balance;\r\n    }\r\n\r\n    function getBalances(address walletAddress, address[] memory tokenAddresses) public view returns (Amount[] memory) {\r\n        Amount[] memory tokenAmounts;\r\n        for (uint64 i = 0; i < tokenAddresses.length; i++) {\r\n            address token = tokenAddresses[i];\r\n            uint256 balance = ERC20(token).balanceOf(walletAddress);\r\n            Amount memory tokenAmount = Amount(token, balance);\r\n            tokenAmounts[i] = tokenAmount; \r\n        }\r\n        return tokenAmounts;\r\n    }\r\n}",
